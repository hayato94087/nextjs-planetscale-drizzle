{
  "name": "nextjs-planetscale-drizzle",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "pnpm drizzle-kit generate:mysql --config drizzle.config.ts",
    "db:push": "pnpm with-env node -r esbuild-register src/db/migrate.ts",
    "db:seed": "pnpm with-env node -r esbuild-register src/db/seed.ts",
    "db:select": "pnpm with-env node -r esbuild-register src/db/select.ts",
    "with-env": "dotenv -e .env --"
  },
  "dependencies": {
    "@planetscale/database": "^1.7.0",
    "@types/node": "20.3.1",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "autoprefixer": "10.4.14",
    "drizzle-orm": "^0.26.5",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.5",
    "next": "13.4.5",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3",
    "undici": "^5.22.1"
  },
  "devDependencies": {
    "dotenv-cli": "^7.2.1",
    "drizzle-kit": "^0.18.1",
    "esbuild-register": "^3.4.2"
  }
}
